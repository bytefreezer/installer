---
# ByteFreezer Secrets
# Copy to secrets.yml and encrypt with: ansible-vault encrypt vars/secrets.yml

# Control Service API Key (required)
vault_control_service_api_key: "your-api-key-here"

# S3 Credentials (required if not using MinIO)
vault_s3_access_key: ""
vault_s3_secret_key: ""

# MinIO Credentials (if minio_enabled: true)
vault_minio_root_password: "change-me-in-production"
